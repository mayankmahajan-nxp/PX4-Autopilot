#!/bin/sh
#
# S32K1XX specific defaults
#------------------------------------------------------------------------------

set LOGGER_BUF 8

param set-default -s UAVCAN_ENABLE 1
param set-default -s UAVCAN_NODE_ID 2
uavcan start

if param greater -s UAVCAN_ENABLE 1
then
	# Reduce logger buffer to free up some RAM for UAVCAN servers.
	set LOGGER_BUF 6
fi
